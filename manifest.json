{
  "manifest_version": 3,
  "name": "Easy Copy Figma Node ID",
  "version": "1.0",
  "description": "A Chrome extension to extract the Node ID from a Figma URL into your clipboard. Â©RRD.",
  "permissions": ["tabs", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
