{
    "manifest_version": 3,
    "name": "Easy Copy Figma Node ID",
    "version": "1.0",
    "description": "A Chrome extension to extract the Node ID from a Figma URL into your clipboard. Â©RRD.",
    "permissions": [
      "activeTab",
      "clipboardWrite",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png"
      }
    },
    "icons": {
      "48": "images/icon48.png"
    }
  }
  